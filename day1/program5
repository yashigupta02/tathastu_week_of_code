player1 = int(input("runs scored by player 1:"))
player2 = int(input("runs scored by player 2:"))
player3 = int(input("runs scored by player 3:"))
strike_rate1 = (player1 * 100)/60
strike_rate2 = (player2 * 100)/60
strike_rate3 = (player3 * 100)/60
print("strike rate of player 1 is ",strike_rate1)
print("strike rate of player 2 is ",strike_rate2)
print("strike rate of player 3 is ",strike_rate3)
print("runs scored by player 1 if they played 60 balls more :",player1 * 2)
print("runs scored by player 2 if they played 60 balls more :",player2 * 2)
print("runs scored by player 3 if they played 60 balls more :",player3 * 2)
print("maximum number of sixes player 1 could hit :",player1 // 6)
print("maximum number of sixes player 2 could hit :",player2 // 6)
print("maximum number of sixes player 3 could hit :",player3 // 6)
